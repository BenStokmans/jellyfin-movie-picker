{
  "name": "jellyfin-movie-picker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "dev": "cd server && npm run dev",
    "build:client": "npx tsc -b && vite build",
    "build:server": "cd server && npm run build",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "npm run build && cd server && npm run prod",
    "docker:build": "docker build -t jellyfin-movie-picker .",
    "docker:run": "docker run -p 3000:3000 jellyfin-movie-picker",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build"
  },
  "dependencies": {
    "@jellyfin/sdk": "^0.11.0",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.7.3",
    "firebase-admin": "^13.4.0",
    "http": "^0.0.1-security",
    "lucide-react": "^0.474.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/socket.io": "^3.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
